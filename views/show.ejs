<% include partials/headersub %>
    <div id="main" class="container">
        <h2><%=place.topic%></h2>
        <img src="<%= place.image %>" alt="">
        <hr>
        <div>
            <%= place.describe %>
        </div>
        <hr>
        <% place.comments.forEach(function(comment){ %>
            <p>
                <strong><%= comment.author %></strong> - <%= comment.text %>
            </p>
        <% })  %>
    </div>

<% include partials/footer %>